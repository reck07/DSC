#include<stdio.h>
void main()
{
int a[100],i,n,key,flag=0;
printf("enter number of element");
scanf("%d",&n);

printf("enter %d number",&n);
for(i=0;i<n;i++)
scanf("%d",&a[i]);

printf("enter the number to search");
scanf("%d",&key);
for(i=0;i<n;i++)
if(a[i]=key)
{
flag=1;
break;
}
    if(flag==1)
    printf("number found");
else

printf("nuber not found");
}
